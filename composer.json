{
  "name": "knit-pk/api-v1-php",
  "description": "KNIT API PHP",
  "type": "project",
  "license": "proprietary",
  "prefer-stable": true,
  "minimum-stability": "dev",
  "require": {
    "php": ">= 7.1",
    "symfony/framework-bundle": "^4",
    "nelmio/cors-bundle": "^1.5",
    "api-platform/api-pack": "^1.0",
    "symfony/monolog-bundle": "^3.1",
    "vich/uploader-bundle": "^1.6",
    "friendsofsymfony/user-bundle": "^2.0",
    "javiereguiluz/easyadmin-bundle": "^1.17",
    "lexik/jwt-authentication-bundle": "^2.4",
    "gesdinet/jwt-refresh-token-bundle": "^0.3",
    "symfony/flex": "^1.0",
    "symfony/yaml": "^4",
    "guzzlehttp/guzzle": "^6.3",
    "predis/predis": "^1.1",
    "ramsey/uuid-doctrine": "^1.4",
    "gedmo/doctrine-extensions": "^2.4.32",
    "roave/security-advisories": "dev-master"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/k911/DoctrineExtensions",
      "no-api": true
    },
    {
      "type": "vcs",
      "url": "https://github.com/k911/JWTRefreshTokenBundle",
      "no-api": true
    }
  ],
  "require-dev": {
    "api-platform/schema-generator": "^2.0",
    "doctrine/doctrine-fixtures-bundle": "^2.4",
    "friendsofphp/php-cs-fixer": "^2.9",
    "phpstan/phpstan": "^0.9.1",
    "phpunit/phpunit": "^6.4",
    "symfony/console": "^4",
    "symfony/dotenv": "^4",
    "symfony/profiler-pack": "^1.0",
    "symfony/web-server-bundle": "^4"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": [
      "phpunit",
      "php-cs-fixer fix --dry-run --diff --allow-risky=true",
      "phpstan analyze src tests --level=4"
    ],
    "fix": "php-cs-fixer fix",
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*",
    "symfony/twig-bundle": "<3.3",
    "symfony/debug": "<3.3"
  },
  "extra": {
    "symfony": {
      "id": "01BT637NNPCSQR7VSBRRG0EEW4",
      "allow-contrib": true
    }
  }
}
