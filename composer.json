{
  "type": "project",
  "license": "proprietary",
  "prefer-stable": true,
  "minimum-stability": "dev",
  "require": {
    "php": ">= 7.1",
    "api-platform/api-pack": "^1.0",
    "gedmo/doctrine-extensions": "^2.4",
    "lexik/jwt-authentication-bundle": "^2.4",
    "ramsey/uuid-doctrine": "^1.4",
    "roave/security-advisories": "dev-master",
    "symfony/flex": "^1.0",
    "symfony/framework-bundle": "^3.3",
    "symfony/yaml": "^3.3"
  },
  "require-dev": {
    "javiereguiluz/easyadmin-bundle": "^1.17",
    "phpunit/phpunit": "^6.3",
    "symfony/console": "^3.3",
    "symfony/dotenv": "^3.3",
    "symfony/web-server-bundle": "^3.3"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "auto-scripts": {
      "make cache-warmup": "script",
      "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*",
    "symfony/twig-bundle": "<3.3",
    "symfony/debug": "<3.3"
  },
  "extra": {
    "symfony": {
      "id": "01BT637NNPCSQR7VSBRRG0EEW4",
      "allow-contrib": true
    }
  }
}
